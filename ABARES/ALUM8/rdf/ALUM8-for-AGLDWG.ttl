  skos:notation "5.1.4" ;
  skos:prefLabel "Glasshouses—hydroponic"@en ;
.
<https://linked.data.gov.au/def/abares-alum8/5.1.5>
  a skos:Concept ;
  skos:broader <https://linked.data.gov.au/def/abares-alum8/5.1> ;
