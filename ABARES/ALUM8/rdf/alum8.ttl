.
alum:4.1.1
  a skos:Concept ;
  rdfs:isDefinedBy cs: ;
  skos:broader alum:4.1 ;
  skos:definition "Area managed for hardwood sawlogs or pulpwood."@en ;
  skos:inScheme cs: ;
  skos:notation "4.1.1" ;
  skos:prefLabel "Irrigated hardwood plantation forestry"@en ;
.
alum:4.1.2
  a skos:Concept ;
  rdfs:isDefinedBy cs: ;
  skos:broader alum:4.1 ;
  skos:definition "Area managed for softwood sawlogs or pulpwood."@en ;
  skos:inScheme cs: ;
  skos:notation "4.1.2" ;
