  skos:prefLabel "Airports or aerodromes"@en ;
.
alum:5.7.2
  a skos:Concept ;
  rdfs:isDefinedBy cs: ;
  skos:broader alum:5.7 ;
  skos:definition "Surface prepared for vehicle travel, includes all roads, streets, highways and so on which are larger than the minimum mapping unit. Roadside areas should either be merged with the nearby land use (i.e. road or surrounding land use) or if the area has low levels of disturbance it may be mapped as class 1.3.3 - Residual native cover."@en ;
  skos:inScheme cs: ;
  skos:notation "5.7.2" ;
  skos:prefLabel "Roads"@en ;
.
alum:5.7.3
  a skos:Concept ;
  rdfs:isDefinedBy cs: ;
  skos:broader alum:5.7 ;
  skos:definition "Permanent rail transport tracks and associated infrastructure."@en ;
  skos:inScheme cs: ;
  skos:notation "5.7.3" ;
  skos:prefLabel "Railways"@en ;
.
alum:5.7.4
  a skos:Concept ;
  rdfs:isDefinedBy cs: ;
  skos:broader alum:5.7 ;
  skos:definition "Harbour location where ships dock to transfer people or cargo to or from land."@en ;
  skos:inScheme cs: ;
