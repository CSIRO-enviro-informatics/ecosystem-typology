  skos:prefLabel "Ports and water transport"@en ;
.
alum:5.7.5
  a skos:Concept ;
  rdfs:isDefinedBy cs: ;
  skos:broader alum:5.7 ;
  skos:definition "Includes radar stations, beacons and so on."@en ;
  skos:inScheme cs: ;
  skos:notation "5.7.5" ;
  skos:prefLabel "Navigation and communication"@en ;
.
alum:5.8
  a skos:Concept ;
  rdfs:isDefinedBy cs: ;
  skos:broader alum:5 ;
  skos:definition "Mining and extractive industries. Salt extraction is captured in 6.2.3 - Evaporation basin. Record the type of mining, if known in the commodity field."@en ;
  skos:inScheme cs: ;
  skos:narrower alum:5.8.1 ;
  skos:narrower alum:5.8.2 ;
  skos:narrower alum:5.8.3 ;
  skos:narrower alum:5.8.4 ;
  skos:notation "5.8" ;
  skos:prefLabel "Mining"@en ;
.
alum:5.8.1
  a skos:Concept ;
  rdfs:isDefinedBy cs: ;
  skos:broader alum:5.8 ;
  skos:definition "Land from which minerals, precious stones and coal is being extracted. Includes open cut and deep-shaft mines."@en ;
