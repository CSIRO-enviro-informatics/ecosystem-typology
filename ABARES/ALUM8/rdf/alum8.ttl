  skos:narrower alum:6.2 ;
  skos:narrower alum:6.3 ;
  skos:narrower alum:6.4 ;
  skos:narrower alum:6.5 ;
  skos:narrower alum:6.6 ;
  skos:notation "6" ;
  skos:prefLabel "Water"@en ;
  skos:topConceptOf cs: ;
.
